@model IEnumerable<App.Models.Categoria>

<h2>Lista de Categorías</h2>

<table border="1" cellpadding="5" cellspacing="0">
    <thead>
        <tr>
            <th>Código</th>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var categoria in Model)
        {
            <tr>
                <td>@categoria.CodigoCategoria</td>
                <td>@categoria.Nombre</td>
            </tr>
        }
    </tbody>
</table>
